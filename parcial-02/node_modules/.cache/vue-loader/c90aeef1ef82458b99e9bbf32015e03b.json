 class=\"\" v-if=\"store.auth.id !== null\">\n            <SpinnerLoader v-if=\"estaCargandoEventos\" />\n            <ListadoEventos \n              v-else\n              :eventos=\"eventosOrdenados\"\n              @delete=\"refrescarEventos()\"/>\n            <h2>Amigos</h2>\n            <p class=\"p-4 border text-center alert alert-warning\" role=\"alert\">\n              No tiene amigos ðŸ˜¢\n            </p>\n            <div class=\"d-flex align-items-center mb-2\">\n              <span\n                class=\"mr-2\"\n                style=\"\n                  width: 48px;\n                  height: 48px;\n                  background-color: cornflowerblue;\n                  display: block;\n                  border-radius: 100px;\n                \"\n              ></span>\n              <span\n                class=\"mr-2\"\n                style=\"\n                  width: 48px;\n                  height: 48px;\n                  background-color: cornflowerblue;\n                  display: block;\n                  border-radius: 100px;\n                \"\n              ></span>\n              <span\n                class=\"mr-2\"\n                style=\"\n                  width: 48px;\n                  height: 48px;\n                  background-color: cornflowerblue;\n                  display: block;\n                  border-radius: 100px;\n                \"\n              ></span>\n              <span\n                class=\"mr-2\"\n                style=\"\n                  width: 48px;\n                  height: 48px;\n                  background-color: cornflowerblue;\n                  display: block;\n                  border-radius: 100px;\n                \"\n              ></span>\n              <span\n                class=\"mr-2\"\n                style=\"\n                  width: 48px;\n                  height: 48px;\n                  background-color: cornflowerblue;\n                  display: block;\n                  border-radius: 100px;\n                \"\n              ></span>\n              <span\n                class=\"mr-2\"\n                style=\"\n                  width: 48px;\n                  height: 48px;\n                  background-color: cornflowerblue;\n                  display: block;\n                  border-radius: 100px;\n                \"\n              ></span>\n            </div>\n            <hr />\n            <h2>Mis favoritos</h2>\n            <p class=\"p-4 border text-center alert alert-warning\" role=\"alert\">\n              No tienes favoritos ðŸ’”\n            </p>\n            <a href=\"#\">\n              <div\n                class=\"bg-white p-2 border rounded text-grey d-flex align-items-center mb-2\"\n              >\n                <div class=\"d-flex align-items-center\">\n                  <span\n                    class=\"mr-2\"\n                    style=\"\n                      width: 32px;\n                      height: 32px;\n                      background-color: cornflowerblue;\n                      display: inline-block;\n                      border-radius: 100px;\n                    \"\n                  ></span>\n                </div>\n                <p class=\"m-0 post-fav\">\n                  Esto es un posteo favorito la idea es que haga puntos\n                </p>\n              </div>\n            </a>\n            <hr />\n          </div>\n          <div class=\"\" v-else>\n            <div class=\"p-4 text-center alert alert-primary\" role=\"alert\">\n              <span>\n                <router-link to=\"/login\"><u>Iniciar sesiÃ³n</u></router-link> y\n                desbloquea <b>mÃ¡s</b> opciones</span\n              >\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </section>\n</template>\n\n<script>\nimport store from \"../store\";\nimport ListadoPosteos from \"@/components/ListadoPosteos\";\nimport ListadoEventos from \"@/components/ListadoEventos\";\nimport SpinnerLoader from \"@/components/SpinnerLoader\";\nimport posteosService from \"@/services/posteos\";\nimport eventosService from \"@/services/eventos\";\n\nexport default {\n  name: \"Home\",\n  components: {\n    SpinnerLoader,\n    ListadoPosteos,\n    ListadoEventos\n  },\n  data: function () {\n    return {\n      store,\n      estaCargando: true,\n      estaCargandoEventos: true,\n      posteos: [],\n      eventos: [],\n    };\n  },\n  computed: {\n    posteosOrdenados: function () {\n      const posteos = this.posteos.slice();\n      posteos.reverse();\n      return posteos;\n    },\n    eventosOrdenados: function () {\n      const eventos = this.eventos.slice();\n      eventos.reverse();\n      return eventos;\n    },\n  },\n  methods: {\n    refrescarPosteos() {\n      this.estaCargando = true;\n      posteosService.getAll().then((response) => {\n        this.estaCargando = false;\n        this.posteos = response;\n      });\n    },\n    refrescarEventos() {\n      this.estaCargandoEventos = true;\n      eventosService.getEventosDeUsuario($id).then((response) => {\n        this.estaCargandoEventos = false;\n        this.eventos = response;\n      });\n    },\n  },\n  mounted: function () {\n    this.estaCargando = true;\n    this.estaCargandoEventos = true;\n\n    const $id = store.auth.id;\n\n    console.log('store id', $id);\n\n    posteosService.getAll().then((response) => {\n      this.estaCargando = false;\n      this.posteos = response;\n    });\n\n    eventosService.getEventosDeUsuario($id).then((response) => {\n      this.estaCargandoEventos = false;\n      this.posteos = response;\n    });\n  },\n};\n</script>\n"]}]}