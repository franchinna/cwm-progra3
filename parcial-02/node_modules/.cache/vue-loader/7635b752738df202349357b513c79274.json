nto.titulo}}</h3>\r\n        <p class=\"text-secondary p-0 m-0 f-14\">\r\n          <i class=\"bi bi-geo text-danger\"></i> {{evento.localizacion}}\r\n        </p>\r\n      </div>\r\n    </div>\r\n  </router-link>\r\n</template>\r\n\r\n<script>\r\nimport store from \"@/store\";\r\nimport eventosService from \"@/services/eventos\";\r\nimport { PATH_IMGS } from \"@/constants\";\r\n\r\nexport default {\r\n  name: \"EventosMiniatura\",\r\n  props: [\"evento\"],\r\n  data: function () {\r\n    return {\r\n      store,\r\n    };\r\n  },\r\n  computed: {\r\n    /**\r\n     * @return {String}\r\n     */\r\n    imagenPath() {\r\n      return `${PATH_IMGS}/perfil/${this.evento.imagen}`;\r\n    },\r\n\r\n    /**\r\n     *\r\n     * @return {boolean}\r\n     */\r\n    tieneImagen() {\r\n      return this.evento.imagen !== null && this.evento.imagen !== \"\";\r\n    },\r\n  },\r\n  methods: {\r\n    eliminar(id) {\r\n      eventosService.deleteItem(id).then((success) => {\r\n        if (success) {\r\n          this.store.setStatus({\r\n            titulo: \"Éxito\",\r\n            mensaje: \"El producto se eliminó exitosamente.\",\r\n            tipo: \"success\",\r\n          });\r\n          this.$emit(\"delete\");\r\n        } else {\r\n          this.store.setStatus({\r\n            titulo: \"Error\",\r\n            mensaje: \"Ocurrió un error. El posteo NO se pudo eliminar.\",\r\n            tipo: \"danger\",\r\n          });\r\n        }\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}